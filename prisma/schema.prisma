generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Tokens {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  token           String? 
  data_cadastro   DateTime? @default(now())
}

model Registros {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  name          String?  @unique
  is_open       String?
  ip_placa      String?
  updateAt      DateTime? @default(now())
  scheduled_at  DateTime?
}
